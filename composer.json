{
    "name": "idno/known",
    "description": "A social publishing platform.",
    "version": "1.3.1",
    "type": "project",
    "homepage": "https://withknown.com/",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-Reflection": "*",
        "ext-session": "*",
        "ext-SimpleXML": "*",
        "ext-openssl": "*",
        "ext-gettext": "*",
        "mapkyca/php-ogp": "^1.0",
        "psr/log": "1.1.4",
        "idno/torophp": "^3.0",
        "mf2/mf2": "0.5.0",
        "swiftmailer/swiftmailer": "6.3.0",
        "forkawesome/fork-awesome": "1.2",
        "twbs/bootstrap": "3.4.1",
        "ezyang/htmlpurifier": "4.14.0",
        "rmm5t/jquery-timeago": "1.6.7",
        "tinymce/tinymce": "6.0.1",
        "symfony/event-dispatcher": "4.4.37",
        "symfony/console": "4.4.40",
        "symfony/http-foundation": "5.4.6",
        "symfony/cache": "5.4.7",
        "simplepie/simplepie": "1.5.8",
